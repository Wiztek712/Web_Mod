{
  "manifest_version": 2,
  "name": "Accueil Personnalis√©",
  "version": "1.0",
  "description": "Une page d'accueil dynamique pour afficher les favoris.",
  "permissions": [
    "bookmarks"
  ],
  
  "chrome_url_overrides": {
    "newtab": "app/index.html"
  },
  "browser_action": {
    "default_popup": "app/index.html"
  },
  "web_accessible_resources": [
    "images/*.jpeg"
  ]
}