{
  "manifest_version": 3,
  "name": "Accueil Personnalis√©",
  "version": "1.0",
  "description": "Une page d'accueil dynamique pour afficher les favoris.",
  "permissions": [
    "bookmarks"
  ],
  
  "chrome_url_overrides": {
    "newtab": "app/index.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["images/*.jpeg"],
      "matches": ["<all_urls>"]
  }
  ]
}